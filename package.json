{
  "name": "nsrace",
  "version": "0.3.0",
  "description": "Runs a race, lookups the fastest IP of any domain from different DNS servers",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./types/index.d.js",
  "bin": {
    "nsrace": "lib/bin/nsrace",
    "nsrace-add": "lib/bin/nsrace-add",
    "nsrace-list": "lib/bin/nsrace-list",
    "nsrace-remove": "lib/bin/nsrace-remove",
    "nsrace-run": "lib/bin/nsrace-run"
  },
  "files": [
    "lib",
    "types"
  ],
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imyelo/nsrace.git"
  },
  "author": "yelo <zhihuzeye@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imyelo/nsrace/issues"
  },
  "homepage": "https://github.com/imyelo/nsrace#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "cli-table": "^0.3.11",
    "commander": "^9.4.0",
    "conf": "^10.1.2",
    "dns2": "^2.0.2",
    "got": "^12.1.0",
    "just-flatten-it": "^5.0.1",
    "just-remove": "^3.0.1",
    "just-unique": "^4.0.1",
    "native-dns": "^0.7.0",
    "ora": "^6.1.2",
    "p-all": "^4.0.0",
    "p-timeout": "^5.1.0",
    "performance-now": "^2.1.0",
    "tcp-ping": "^0.1.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/tcp-ping": "^0.1.3",
    "@types/text-table": "^0.2.2",
    "@yelo/eslint-config": "^0.0.3",
    "@yelo/prettier-config": "^0.0.1",
    "@yelo/tsconfig": "^0.0.2",
    "eslint": "^8.20.0",
    "gulp": "^4.0.2",
    "gulp-chmod": "^3.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "clean": "rimraf ./lib ./types",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && node ./build/chmod-bin.js",
    "lint": "prettier-eslint --write $PWD/\"src/**/*.[jt]s?(x)\" "
  },
  "prettier": "@yelo/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
